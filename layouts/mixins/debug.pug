
mixin DebugObject(object)
	table
		each val, key in object
			tr
				td= key
				if typeof val == "object"
					td
						+DebugObject(val)
				else
					td= val

mixin DebugAnyProps(props)
	+DebugObject(props)

mixin DebugProps()
	h2 Props
	+DebugAnyProps(props)

mixin DebugCProps()
	h2 CProps
	+DebugAnyProps(cprops)

mixin DebugSProps()
	h2 SProps
	+DebugAnyProps(sprops)

mixin DebugKProps()
	h2 KProps
	+DebugAnyProps(kprops)

mixin DebugTProps()
	h2 TProps
	+DebugAnyProps(tprops)


